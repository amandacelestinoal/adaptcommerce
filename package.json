{
  "name": "adaptcommerce",
  "version": "1.0.0",
  "description": "Prototype your next eCommerce experiences using modern web technologies",
  "scripts": {
    "lint": "eslint --max-warnings=0 --fix \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "watch:server": "NODE_ENV=development webpack --watch --config webpack.config.js --config-name server",
    "dev:server": "NODE_ENV=development nodemon build/server.js",
    "dev:client": "NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "dev:docs": "docsify serve ./docs",
    "start": "concurrently -k \"npm run watch:server\" \"npm run dev:server\" \"npm run dev:client\"",
    "serve": "NODE_ENV=production node build/server/server.js",
    "now-start": "npm run serve",
    "storybook": "start-storybook -p 9001 -s .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adaptcommerce/adaptcommerce.git"
  },
  "keywords": [
    "ecommerce",
    "react",
    "graphql",
    "apollo",
    "pwa",
    "magento2"
  ],
  "author": "adaptcommerce",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adaptcommerce/adaptcommerce/issues"
  },
  "homepage": "https://github.com/adaptcommerce/adaptcommerce#readme",
  "dependencies": {
    "@gramps/gramps": "2.0.0",
    "@moltin/sdk": "3.19.0",
    "apollo-boost": "0.3.1",
    "apollo-client": "2.5.1",
    "apollo-link-http": "1.5.14",
    "apollo-server-express": "2.4.8",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "faker": "4.1.0",
    "fetch-cookie": "0.7.2",
    "file-loader": "3.0.1",
    "graphql": "14.1.1",
    "graphql-tools": "4.0.4",
    "node-fetch": "2.3.0",
    "raw-loader": "2.0.0",
    "react": "16.8.4",
    "react-apollo": "2.5.2",
    "react-dom": "16.8.4",
    "react-modal": "3.8.1",
    "react-router-dom": "5.0.0",
    "recompose": "0.30.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.0",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-actions": "5.0.3",
    "@storybook/react": "5.0.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-polyfill": "6.26.0",
    "concurrently": "4.1.0",
    "css-loader": "2.1.1",
    "docsify-cli": "4.3.0",
    "eslint": "5.16.0",
    "eslint-config-react-app": "3.0.8",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-app": "4.0.1",
    "graphql-tag": "2.10.1",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.11.0",
    "nodemon": "1.18.10",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "prettier": "1.16.4",
    "sass-loader": "7.1.0",
    "storybook-readme": "5.0.1",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1",
    "webpack-node-externals": "1.7.2"
  }
}
